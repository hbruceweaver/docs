{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Higgsfield API",
  "colors": {
    "primary": "#6366F1",
    "light": "#818CF8",
    "dark": "#4F46E5"
  },
  "favicon": "/favicon.svg",
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "Getting Started",
            "pages": ["introduction", "quickstart", "authentication"]
          },
          {
            "group": "Guides",
            "pages": [
              "guides/text-to-video",
              "guides/image-to-video",
              "guides/motion-control",
              "guides/video-editing",
              "guides/text-to-image",
              "guides/image-enhancement",
              "guides/webhooks"
            ]
          },
          {
            "group": "Models",
            "pages": [
              "models/overview",
              "models/kling-26",
              "models/kling-motion-control",
              "models/kling-o1-edit",
              "models/nano-banana-2",
              "models/soul",
              "models/seedream",
              "models/skin-enhancer",
              "models/topaz-upscale"
            ]
          },
          {
            "group": "Concepts",
            "pages": [
              "concepts/jobs",
              "concepts/accounts",
              "concepts/webhooks",
              "concepts/credits",
              "concepts/rate-limits"
            ]
          },
          {
            "group": "SDKs",
            "pages": ["sdks/overview", "sdks/typescript"]
          },
          {
            "group": "Resources",
            "pages": ["changelog"]
          }
        ]
      },
      {
        "tab": "API Reference",
        "groups": [
          {
            "group": "Overview",
            "pages": ["api-reference/introduction", "api-reference/errors"]
          },
          {
            "group": "Jobs",
            "pages": [
              "api-reference/jobs/create",
              "api-reference/jobs/get",
              "api-reference/jobs/list",
              "api-reference/jobs/cancel",
              "api-reference/jobs/retry"
            ]
          },
          {
            "group": "Accounts",
            "pages": [
              "api-reference/accounts/list",
              "api-reference/accounts/create",
              "api-reference/accounts/disable",
              "api-reference/accounts/enable",
              "api-reference/accounts/update-auth"
            ]
          },
          {
            "group": "Stats",
            "pages": ["api-reference/stats/overview"]
          },
          {
            "group": "Pricing",
            "pages": [
              "api-reference/pricing/models",
              "api-reference/pricing/calculate"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "GitHub",
          "href": "https://github.com/higgsfield-ai",
          "icon": "github"
        },
        {
          "anchor": "Discord",
          "href": "https://discord.gg/higgsfield",
          "icon": "discord"
        }
      ]
    }
  },
  "navbar": {
    "links": [
      {
        "label": "Dashboard",
        "href": "https://higgsfield-api.vercel.app"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Get API Key",
      "href": "https://higgsfield-api.vercel.app/api-keys"
    }
  },
  "footer": {
    "socials": {
      "x": "https://x.com/higgsfield_ai",
      "github": "https://github.com/higgsfield-ai",
      "discord": "https://discord.gg/higgsfield"
    }
  },
  "api": {
    "baseUrl": "https://higgsfield-api.vercel.app",
    "auth": {
      "method": "bearer"
    }
  }
}
